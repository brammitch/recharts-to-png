{
  "name": "recharts-to-png",
  "version": "2.0.2",
  "description": "This package converts a Recharts chart to a png.",
  "author": "Brandon M. Mitchell",
  "license": "MIT",
  "homepage": "https://github.com/brammitch/recharts-to-png#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/brammitch/recharts-to-png"
  },
  "bugs": {
    "url": "https://github.com/brammitch/recharts-to-png/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "recharts",
    "svg",
    "download",
    "image",
    "save",
    "png",
    "react",
    "typescript"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx rollup -c rollup.config.js && cp ./dist/index.d.ts ./index.d.ts",
    "release": "standard-version",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "coverage": "jest --coverage",
    "format:check": "prettier --check \"./**/*.{ts,tsx,js,json}\"",
    "format:fix": "prettier --write \"./**/*.{ts,tsx,js,json}\"",
    "lint:check": "eslint . --ext .js,.ts,.tsx --color",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --color --fix",
    "prepare": "husky install",
    "watch": "npx rollup -c rollup.config.js -w",
    "demo": "npm --prefix ./demo/ start"
  },
  "peerDependencies": {
    "react": ">=16.8.3",
    "react-dom": ">=16.8.3",
    "recharts": ">=1.8.0"
  },
  "dependencies": {
    "html2canvas": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@rollup/plugin-babel": "5.3.1",
    "@testing-library/react": "13.2.0",
    "@types/file-saver": "2.0.5",
    "@types/jest": "27.5.1",
    "@types/node": "16.11.36",
    "@types/random-number": "*",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "babel-jest": "27.5.1",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "file-saver": "2.0.5",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-canvas-mock": "2.4.0",
    "prettier": "2.6.2",
    "random-number": "0.0.9",
    "react": "18.1.0",
    "react-app-rewired": "2.2.1",
    "react-dom": "18.1.0",
    "react-scripts": "5.0.1",
    "recharts": "2.1.10",
    "rollup": "2.74.1",
    "rollup-plugin-typescript2": "0.31.2",
    "standard-version": "9.5.0",
    "ts-jest": "27.1.5",
    "typescript": "4.6.4"
  }
}
