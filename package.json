    "@types/recharts": "^1.8.14",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "html2canvas": "^1.0.0-rc.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
